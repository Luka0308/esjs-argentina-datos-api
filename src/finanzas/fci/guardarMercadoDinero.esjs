importar { escribirRuta } desde '@/utils/rutas.esjs'
importar { leerRuta } desde '@/utils/rutas.esjs'
importar { format } desde 'date-fns'

exportar asincrono funcion guardarMercadoDinero(items, fecha) {
  const fechaConBarra = format(fecha, 'yyyy/MM/dd')

  retornar escribirRuta(`/finanzas/fci/mercadoDinero/${fechaConBarra}`, items)
}
